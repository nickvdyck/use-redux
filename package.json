{
  "name": "@nvd/use-redux",
  "version": "1.1.0",
  "description": "React hook utility functions to access state and dispatch actions from a redux store.",
  "license": "MIT",
  "author": {
    "name": "Nick Van Dyck",
    "url": "https://nvd.codes"
  },
  "repository": "nickvdyck/use-redux",
  "main": "./lib",
  "module": "./es/index.js",
  "scripts": {
    "build": "yarn run clean && yarn build-cjs && yarn build-es",
    "build-cjs": "tsc -p tsconfig.json",
    "build-es": "tsc -p tsconfig.json --outDir es --module esnext",
    "clean": "del-cli dist es",
    "lint": "eslint --ext .ts,.tsx .",
    "prepublish": "yarn run build",
    "test": "yarn run lint && yarn run test-coverage",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "files": [
    "es",
    "lib"
  ],
  "devDependencies": {
    "@nvd/tsconfig": "1.0.0",
    "@testing-library/react": "9.1.4",
    "@testing-library/react-hooks": "2.0.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.4",
    "@types/react": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "del-cli": "2.0.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "redux": "4.0.4",
    "ts-jest": "24.0.2",
    "typescript": "3.6.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "redux": "^4.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintIgnore": [
    "lib",
    "es"
  ]
}
